{
    "name": "Global Tab Management Shortcuts",
    "version": "0.9",
    "manifest_version": 2,
    "icons": {
        "128": "new-tab.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "tabs"
    ],
    "commands": {
        "new-tab": {
            "description": "New tab"
        },
        "next-tab": {
            "description": "Next tab"
        },
        "prev-tab": {
            "description": "Prev tab"
        },
        "close-tab": {
            "description": "Close tab"
        }
    }
}
